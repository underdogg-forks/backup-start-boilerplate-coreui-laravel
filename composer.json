{
    "name": "adr1enbe4udou1n/laravel-boilerplate",
    "description": "Laravel 5.5 Boilerplate based on Bootstrap 4 (beta) and Vue CoreUI for Backend.",
    "keywords": ["framework", "laravel", "boilerplate", "bootstrap 4 beta", "vue", "coreui"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1",
        "ext-gd": "*",
        "arcanedev/no-captcha": "^5.0",
        "artesaos/seotools": "^0.11.1",
        "cviebrock/eloquent-sluggable": "^4.2",
        "cviebrock/eloquent-taggable": "^3.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "dimsav/laravel-translatable": "^7.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.2",
        "hieu-le/active": "^3.3",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "^2.1",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.16",
        "mcamara/laravel-localization": "^1.3",
        "mews/purifier": "^2.0",
        "plank/laravel-mediable": "^2.5",
        "renatomarinho/laravel-page-speed": "^1.8",
        "roumen/sitemap": "^2.6",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
        "yajra/laravel-datatables-oracle": "^8.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
